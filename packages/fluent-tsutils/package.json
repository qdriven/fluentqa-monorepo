{
  "name": "@fluentqa/tsutils",
  "version": "0.0.1",
  "description": "shared typescript utilities",
  "main": "dist/index.cjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "run-it": "esno src/index.ts",
    "coverage": "vitest run --coverage"
  },
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
    "dprint": "^0.34.1",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "got": "^12.5.3",
    "gray-matter": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.2",
    "p-throttle": "^5.0.0",
    "ts-pattern": "^4.0.6",
    "tsup": "^6.5.0",
    "vite": "^4.0.3",
    "vitest": "^0.26.2"
  }
}
